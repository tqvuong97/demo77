<h1>New Post</h1>

 <%#render 'form' %>

<%= link_to 'Back', posts_path %>




<% form_for @post do |f| %>
  <%= f.error_messages %>
  <%= render "steps/#{@post.current_stepp}", :f => f %>
  <p><%= f.submit "Continue" %></p>
  <p><%= f.submit "Back", :name => "back_button" unless @post.first_step? %></p>
<% end %>





